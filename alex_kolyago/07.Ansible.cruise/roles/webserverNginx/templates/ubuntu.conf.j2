server {
    listen       80;
    server_name  {{ site_deb }} www.{{ site_deb }};
    root {{ default_home }};

    access_log /var/log/nginx/{{ site_deb }}_access_log;
    error_log /var/log/nginx/{{ site_deb }}_error_log;


    location ~* ^.+\.(jpg|jpeg|gif|png|css|zip|tgz|gz|rar|bz2|doc|docx|xls|xlsx|exe|pdf|ppt|tar|wav|bmp|rtf|js)$ {
            expires modified +1w;
    }
}
